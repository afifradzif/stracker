{
  "expo": {
    "name": "Stracker",
    "slug": "stracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/stracker-square.png",
    "scheme": "stracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qagura.stracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/stracker-square.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.qagura.stracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logotransparent1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "896406b4-87f2-4ac3-b075-b56014629a30"
      }
    }
  }
}
